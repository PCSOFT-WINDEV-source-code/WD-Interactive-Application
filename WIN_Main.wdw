     procedure_id : 1643862689183910094
     type_code : 14
     code : |1+
      // Summary: Refreshes the temperatures
      // 
      // Automatic procedure:
      // The procedure is automatically run, after the initialization code of the window, with a delay set to 1 second
      // It will be repeated in loop, with 1 second between each call
      // Each following call runs the procedure once, without timer
      // 
      
      PROCEDURE RefreshTemperatures()
      
      
      FOR i = 1 _TO_ 4 
      	// Initializes the generator of random numbers
      	InitRandom()
      	
      	xRandomNumber is numeric
      	xRandomNumber = Random(190, 260) / 10
      	
      	// Rounded
      	xRandomNumber = Round(xRandomNumber, 1)
      	
      	{"IWC_Edit.STC_Temperature_" + i,indControl} = xRandomNumber
      END
     type : 458752
